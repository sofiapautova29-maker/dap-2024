        "\n",
        "print(f\"DataFrame с индексами {df_test.index.tolist()}:\")\n",
        "print(df_test)\n",
        "print(f\"\\nSeries с индексами {s_test.index.tolist()}:\")\n",
        "print(s_test)\n",
        "\n",
        "print(\"\\nПопытка добавить Series как столбец:\")\n",
        "try:\n",
        "    df_test['B'] = s_test\n",
        "    print(\"Успешно! Но значения не совпадут по индексам\")\n",
        "    print(df_test)\n",
        "except Exception as e:\n",
        "    print(f\"Проблема: {e}\")\n",
        "\n",
        "print(\"\\nРешение: переиндексация Series:\")\n",
        "s_test_reindexed = s_test.reindex(df_test.index)\n",
